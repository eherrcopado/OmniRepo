&lt;script type=&quot;text/ng-template&quot; id=&quot;vlcSideBar.html&quot;&gt;
	&lt;div class=&quot;vlc-slds-cart&quot; ng-if=&quot;bpTree.showPC&quot;&gt;
	&lt;ng-include ng-repeat=&quot;vlcPC in ::bpTree.propSetMap.persistentComponent&quot;
	ng-if=&apos;bpTree.showPCNew[vlcPC.pcIndex] &amp;&amp; ($index === 0 )&apos;
	id=&apos;{{::vlcPC.id}}&apos;
	class=&quot;slds-box&quot;
	src=&quot;::bpTree.dfHPSTmpl[vlcPC.pcIndex]&quot;/&gt;
	&lt;/div&gt;
	&lt;div ng-include=&quot;&apos;vlcStepChart.html&apos;&quot; style=&quot;width: 100%;&quot;&gt;&lt;/div&gt;
&lt;/script&gt;

&lt;script type=&quot;text/ng-template&quot; id=&quot;vlcStepChart.html&quot;&gt;
	&lt;section class=&quot;slds-page-header slds-m-top_x-small&quot; ng-if=&quot;::!bpTree.propSetMap.hideStepChart&quot;&gt;
		&lt;div class=&apos;slds-size_1-of-1&apos;&gt;
			&lt;div class=&quot;slds-size_6-of-12 slds-align_absolute-center slds-grid&quot; &gt;
				&lt;div class=&quot;slds-size_1-of-1 slds-col_bump-right&quot; style=&quot;width: 100%;&quot;&gt;
					&lt;div class=&quot;slds-progress slds-size_1-of-1&quot; ng-init=&quot;stepsCnt=[]&quot;&gt;
						&lt;ol class=&quot;slds-progress__list&quot;&gt;
						&lt;li class=&quot;slds-progress__item slds-is-relative&quot;
						ng-class=&quot;{&apos;slds-is-active&apos;: step.bAccordionActive, &apos;slds-is-completed&apos; : (!step.bAccordionActive &amp;&amp; step.completed), &apos;slds-has-error&apos; : (!step.bAccordionActive &amp;&amp; step.tInvalid &amp;&amp; step.visited)}&quot;
						ng-repeat=&quot;step in ::children&quot;
						ng-if=&quot;step.type === &apos;Step&apos; &amp;&amp; step.show&quot;
						state=&quot;{{step.bAccordionActive}}&quot;
						ng-click=&quot;sidebarNav(step)&quot;
						ng-init=&quot;stepsCnt.push(0)&quot;
						vlc-slds-step-styler&gt;
							&lt;button class=&quot;slds-button slds-progress__marker&quot; ng-if=&quot;step.completed &amp;&amp; !step.bAccordionActive&quot;&gt;
								&lt;slds-svg-icon aria-hidden=&quot;true&quot;
								sprite=&quot;&apos;utility&apos;&quot;
								icon=&quot;&apos;success&apos;&quot;
								size=&quot;&apos;small&apos;&quot;
								extra-classes=&quot;&apos;slds-button__icon&apos;&quot;&gt;
								&lt;/slds-svg-icon&gt;
							&lt;/button&gt;
							&lt;button class=&quot;slds-button slds-progress__marker&quot; ng-if=&quot;!step.bAccordionActive &amp;&amp; step.tInvalid &amp;&amp; step.visited&quot;&gt;
								&lt;slds-svg-icon aria-hidden=&quot;true&quot;
								sprite=&quot;&apos;utility&apos;&quot;
								icon=&quot;&apos;warning&apos;&quot;
								size=&quot;&apos;small&apos;&quot;
								extra-classes=&quot;&apos;slds-button__icon&apos;&quot;&gt;
								&lt;/slds-svg-icon&gt;
							&lt;/button&gt;
							&lt;button class=&quot;slds-button slds-progress__marker&quot; ng-if=&quot;step.bAccordionActive || (!step.bAccordionActive &amp;&amp; !step.completed)&quot;
							ng-hide=&quot;!step.bAccordionActive &amp;&amp; step.tInvalid &amp;&amp; step.visited&quot;&gt;
							&lt;/button&gt;
							&lt;div ng-if=&quot;step.bAccordionActive&quot; class=&quot;slds-is-absolute&quot;&gt;
								&lt;div class=&quot;slds-m-top_medium slds-is-relative slds-progress__step-label&quot;&gt;
									&lt;div&gt;{{step.propSetMap.label}}&lt;/div&gt;
								&lt;/div&gt;
							&lt;/div&gt;
							&lt;div ng-if=&quot;!step.bAccordionActive&quot; class=&quot;slds-is-absolute&quot; &gt;
								&lt;div class=&quot;slds-popover slds-popover--tooltip slds-nubbin--top&quot; role=&quot;tooltip&quot;&gt;
									&lt;div class=&quot;slds-popover__body&quot;&gt;{{step.propSetMap.label}}&lt;/div&gt;
								&lt;/div&gt;
							&lt;/div&gt;
						&lt;/li&gt;
					&lt;/ol&gt;
						&lt;div class=&quot;slds-progress-bar slds-progress-bar_x-small&quot; aria-valuemin=&quot;0&quot; aria-valuemax=&quot;100&quot; aria-valuenow=&quot;0&quot; role=&quot;progressbar&quot; ng-if=&quot;stepsCnt.length &gt; 1&quot;&gt;
							&lt;span class=&quot;slds-progress-bar__value&quot; ng-style=&quot;{&apos;width&apos;:calculateProgressWidth(), &apos;background&apos;: &apos;#5eb4ff&apos;}&quot;&gt;
							&lt;span class=&quot;slds-assistive-text&quot;&gt;Progress: 0%&lt;/span&gt;
							&lt;/span&gt;
						&lt;/div&gt;
					&lt;/div&gt;
				&lt;/div&gt;
			&lt;/div&gt;
		&lt;/div&gt;
	&lt;/section&gt;
&lt;/script&gt;

&lt;script type=&quot;text/ng-template&quot; id=&quot;vlcSldsRightPanel.html&quot;&gt;
	&lt;div class=&quot;vlc-slds-right--panel&quot; ng-if=&quot;::!bpTree.propSetMap.hideStepChart&quot;&gt;
